<body id="newform">
	<div id='form'>
		<h1 id='h1'>Sign in to your account.</h1>
	  <%= form_tag sessions_path  %>

	    <p><%= text_field_tag :email, "", id: 'input' , placeholder: 'Email' %></p>
	    <p><%= password_field_tag :password, "", id: 'input', placeholder: 'Password' %></p>
	    <%= submit_tag "Submit", id: 'usersubmit'  %>

	  </div>
	</div>
</body>